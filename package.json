{
  "name": "kaleabmelkie",
  "version": "3.1.0",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "del-cli .svelte-kit build",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src \"*.{cjs,js,json,md}\" && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --check --write --plugin-search-dir=. src \"*.{cjs,js,json,md}\" && eslint --fix --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "autoprefixer": "10.4.1",
    "del-cli": "4.0.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.1",
    "husky": "7.0.4",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "prettier-plugin-svelte": "2.5.1",
    "rss-to-json": "2.0.2",
    "svelte": "3.44.3",
    "svelte-check": "2.2.11",
    "svelte-preprocess": "4.10.1",
    "tailwindcss": "3.0.8",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "workbox-precaching": "6.4.2"
  },
  "type": "module",
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
